#include<iostream>
using namespace std;
void pattern1(int n, int m){
    for(int i = 0 ; i<n ;i++){
        for(int j = 0; j<m; j++){
            cout<<"*";
        }
        cout<<endl;
    }
    
}
int main(){
    int n ,  m;
    cout<<"Enter the number n : ";
    cin>>n;
    cout<<"Enter the number m: ";
    cin>>m;
    pattern1(n,m);
}
